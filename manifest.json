{
  "manifest_version": 2,

  "name": "vkmin",
  "description": "remove some bullshit from vk.com",
  "version": "1.0",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "activeTab"
   ],
   "content_scripts": [
       {
         "matches": ["https://vk.com/*"],
         "js": ["prototype.js"]
       }
     ]

}
